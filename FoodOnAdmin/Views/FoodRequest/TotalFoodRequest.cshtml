                    <div id="AddAdminReg_modal" class="modal fade" data-backdrop="static" data-keyboard="false" role="dialog">
                        <div class="modal-dialog modal-md" style="font-family:Roboto;">
                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-header" style="background-image: linear-gradient(to right,#13b757f2,#eba60a); color: black;padding:10px;">
                                    <span class="modal-title"><b>Add Product</b></span>
                                    <button type="button" class="close" data-dismiss="modal" style="margin-right:5px;">&times;</button>

                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <form name="C2" ng-submit="C2(C2.$valid)" novalidate" data-toggle="validator" role="form">
                                                <label hidden>{{CITY_ID}}</label>
                                                <div class="row">
                                                    <div class="col-lg-12 form-group">
                                                        <label style="font-size:11px;">Product Name :</label><label style="color:red;">*</label>
                                                        <input type="text" name="AREA_NAME" class="form-control" ng-model="AREA_NAME" maxlength="120" required placeholder="Enter Product Name" />
                                                    </div>
                                                    <div class="col-lg-12 form-group">
                                                        <label style="font-size:11px;">Select Food Type :</label><label style="color:red;">*</label>
                                                        <input type="text" name="AREA_NAME" class="form-control" ng-model="AREA_NAME" maxlength="120" required placeholder="Enter Food Type" />
                                                    </div>
                                                    <div class="col-lg-12 form-group">
                                                        <label style="font-size:11px;">Price :</label><label style="color:red;">*</label>
                                                        <input type="text" name="AREA_NAME" class="form-control" ng-model="AREA_NAME" maxlength="120" required placeholder="Enter Area Name" />
                                                    </div>
                                                    <div class="col-lg-12 form-group">
                                                        <label style="font-size:11px;">Discount :</label><label style="color:red;">*</label>
                                                        <input type="text" name="AREA_NAME" class="form-control" ng-model="AREA_NAME" maxlength="120" required placeholder="Enter Area Name" />
                                                    </div>
                                                    <div class="row">

                                                        <div class="col-lg-12">
                                                            <div class="form-group">
                                                                <label style="color:black;font-size:11px;">Product Image:</label><label style="color:red">*</label>
                                                                <input type="file" ng-model="IMAGE_URL" class="form-control" accept="image/*" style="height:38px;" id="Profile_photo">
                                                                <label style="color:green;font-size:14px;">Note:Image size should be less than 500 kb and Dimension: 512 * 220 pixels.</label>
                                                                <a href="#" style="display:none" id="divProfile">
                                                                    <img src={{IMAGE_URL}} style="height: 100px; width: 100px; object-fit: contain;">&nbsp; &nbsp; &nbsp;
                                                                </a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <hr>
                                                <div class="row">
                                                    <div class="form-group col-lg-12">
                                                        <center>
                                                            <input type="button" class="btn btn-sm" style="background-color: #13b757f2; color: black; font-size: 14px;" ng-disabled="C2.$invalid" ng-click="AddUpdateAdmin()" value="Add Product" />
                                                        </center>
                                                    </div>
                                                </div>
                                            </form>
