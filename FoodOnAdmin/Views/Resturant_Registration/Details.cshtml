<style>
    .paper .b-r-cta {
        border: 1px solid #17a2b8;
        background-color: #17a2b8;
        background-image: linear-gradient(180deg,#544427,#17a2b8);
        text-shadow: 0 1px 1px rgba(0,0,0,.2);
        padding: 5px 15px;
    }

        .paper .b-r-cta:focus, .paper .b-r-cta:hover {
            transition: all .3s ease;
            background-image: linear-gradient(180deg,#544427,#e86700);
        }

    .paper * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
        /*background-image: linear-gradient(180deg,orange,#ff7302);*/
    }

    .invalid-blink {
        background-color: green;
        color: white;
    }
</style>
<style>
    .catStyle_Desc {
        color: orangered;
        cursor: pointer;
        font-size: 15px;
        border: 1px solid #077F7B;
        padding: 5px 10px;
        border-radius: 5px;
    }

    .catStyle_active_Desc {
        color: white;
        background-color: #077F7B;
        cursor: pointer;
        font-size: 15px;
        font-weight: bold;
        padding: 5px 10px;
        border: 1px solid #077F7B;
        border-radius: 5px;
    }
</style>

<style>
    .table td, .table th {
        padding: 0.30rem;
        vertical-align: top;
        border-top: 1px solid #dee2e6;
        font-size: 12px;
    }
</style>
<style>
    input[type=checkbox] + label {
        display: block;
        margin: 0.2em;
        cursor: pointer;
        padding: 0.2em;
    }

    input[type=checkbox] {
        display: none;
    }

        input[type=checkbox] + label:before {
            content: "\2714";
            border: 0.1em solid #F1511B;
            border-radius: 0.2em;
            /*border-radius: 50%;*/
            display: inline-block;
            width: 1.5em;
            height: 1.5em;
            padding-left: 0.2em;
            padding-bottom: 1.5em;
            margin-right: 0.2em;
            vertical-align: middle;
            color: transparent;
            transition: .2s;
        }

        input[type=checkbox] + label:active:before {
            transform: scale(0);
        }

        input[type=checkbox]:checked + label:before {
            background-color: #F1511B;
            border-color: #F1511B;
            color: #fff;
        }

        input[type=checkbox]:disabled + label:before {
            transform: scale(1);
            border-color: #aaa;
        }

        input[type=checkbox]:checked:disabled + label:before {
            transform: scale(1);
            background-color: #F1511B;
            border-color: #F1511B;
        }
</style>


